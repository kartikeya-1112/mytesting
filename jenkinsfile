pipeline {
    agent any

    tools {
        // Define Maven tool with the specified name
        maven 'maven' // This should match the name you specified in the Maven installation configuration
    }

    stages {
        stage('Checkout') {
            steps {
                // Checkout code from repository
                git url: 'https://github.com/kartikeya-1112/mytesting', branch: 'main'
            }
        }

        stage('Build') {
            steps {
                // Compile Java code using Maven
                sh 'mvn compile'
            }
        }

        // Add other stages as needed
    }
}
