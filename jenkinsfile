pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                // Checkout code from repository
                git 'https://github.com/kartikeya-1112/mytesting'
            }
        }

        stage('Build') {
            steps {
                // Compile Java code using Maven
                sh 'mvn compile'
            }
        }
    }
}
