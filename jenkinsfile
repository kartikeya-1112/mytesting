node {
    stage('Checkout') {
        // Checkout code from repository
        git url: 'https://github.com/kartikeya-1112/mytesting', branch: 'main'
    }

    stage('Build') {
        // Compile Java code using Maven
        sh 'mvn compile'
    }

    stage('Test') {
        // Run tests using Maven
        sh 'mvn test'
    }

    stage('Package') {
        // Package the application using Maven
        sh 'mvn package'
    }

    stage('Deploy') {
        // Deploy the application (example step)
        echo 'Deploying the application...'
        // Add your deployment commands here
    }
}
